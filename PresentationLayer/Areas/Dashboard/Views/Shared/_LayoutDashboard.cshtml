
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>DashRock - Bootstrap 4 Admin Dashboard Template</title>
    <!--favicon-->
    <link rel="icon" href="~/Content/color-admin/assets/images/favicon.ico" type="image/x-icon">
    <!-- notifications css -->
    <link rel="stylesheet" href="~/Content/color-admin/assets/plugins/notifications/css/lobibox.min.css" />
    <!-- Vector CSS -->
    <link href="~/Content/color-admin/assets/plugins/vectormap/jquery-jvectormap-2.0.2.css" rel="stylesheet" />
    <!--Full Calendar Css-->
    <link href='~/Content/color-admin/assets/plugins/fullcalendar/css/fullcalendar.css' rel='stylesheet' />
    <!-- simplebar CSS-->
    <link href="~/Content/color-admin/assets/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <!-- Bootstrap core CSS-->
    <link href="~/Content/color-admin/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- animate CSS-->
    <link href="~/Content/color-admin/assets/css/animate.css" rel="stylesheet" type="text/css" />
    <!-- Icons CSS-->
    <link href="~/Content/color-admin/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <!-- Sidebar CSS-->
    <link href="~/Content/color-admin/assets/css/sidebar-menu.css" rel="stylesheet" />
    <!-- Custom Style-->
    <link href="~/Content/color-admin/assets/css/app-style.css" rel="stylesheet" />

</head>

<body onload="info_noti()">

    <!-- Start wrapper-->
    <div id="wrapper">

        <!--Start sidebar-wrapper-->
        <div id="sidebar-wrapper" data-simplebar="" data-simplebar-auto-hide="true">
            <div class="brand-logo">
                <a href="index.html">
                    <img src="~/Content/color-admin/assets/images/logo-icon.png" class="logo-icon" alt="logo icon">
                    <h5 class="logo-text"> DashRock</h5>
                </a>
            </div>
            <div class="user-details">
                @Html.Action("Profile", "Header")
                <div id="user-dropdown" class="collapse">
                    <ul class="user-setting-menu">
                        <li><a href="@Url.Action("Index","Profile")"><i class="icon-user"></i>  My Profile</a></li>
                        <li><a href="@Url.Action("Index","Setting")"><i class="icon-settings"></i> Setting</a></li>
                        <li><a href="@Url.Action("LogOut","Account")"><i class="icon-power"></i> Logout</a></li>
                    </ul>
                </div>
            </div>
            <ul class="sidebar-menu do-nicescrol">
                <li class="sidebar-header">MAIN NAVIGATION</li>
                <li>
                    <a href="@Url.Action("Index","Panel")" class="waves-effect">
                        <i class="icon-home"></i><span>Dashboard</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Authority")" class="waves-effect">
                        <i class="icon-key"></i><span>Authority</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Employee")" class="waves-effect">
                        <i class="icon-user"></i><span>Employee</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Staffs")" class="waves-effect">
                        <i class="icon-people"></i><span>Staffs</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Menus")" class="waves-effect">
                        <i class="icon-cup"></i><span>Menus</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Reservations")" class="waves-effect">
                        <i class="icon-compass"></i><span>Reservations</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Events")" class="waves-effect">
                        <i class="icon-event"></i><span>Events</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Calendar")" class="waves-effect">
                        <i class="icon-calendar"></i><span>Calendar</span>
                    </a>
                </li>

                <li>
                    <a href="javaScript:void();" class="waves-effect">
                        <i class="icon-envelope-letter"></i><span>Mailbox</span>
                        @Html.Partial("~/Areas/Dashboard/Views/Shared/Mail/UnRead.cshtml")
                    </a>
                    <ul class="sidebar-submenu">
                        <li><a href="@Url.Action("Inbox","Mailbox")"><i class="fa fa-long-arrow-right"></i> Inbox</a></li>
                        <li><a href="@Url.Action("Compose","Mailbox")"><i class="fa fa-long-arrow-right"></i> Compose</a></li>
                        <li><a href="@Url.Action("ReadMail","Mailbox")"><i class="fa fa-long-arrow-right"></i> Read Mail</a></li>
                    </ul>
                </li>

                <li>
                    <a href="@Url.Action("Index","About")" class="waves-effect">
                        <i class="icon-globe"></i><span>About</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Address")" class="waves-effect">
                        <i class="icon-location-pin"></i><span>Address</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Banners")" class="waves-effect">
                        <i class="icon-direction"></i><span>Banner</span>
                    </a>
                </li>

                <li>
                    <a href="@Url.Action("Index","Maps")" class="waves-effect">
                        <i class="icon-map"></i><span>Maps</span>
                    </a>
                </li>

                <li class="sidebar-header">LABELS</li>
                <li><a href="javaScript:void();" class="waves-effect"><i class="icon-cloud-upload text-danger"></i><span>Disk Space</span></a></li>
                <li><a href="javaScript:void();" class="waves-effect"><i class="icon-hourglass text-warning"></i><span>Server Load</span></a></li>
                <li><a href="javaScript:void();" class="waves-effect"><i class="icon-drop text-info"></i><span>Information</span></a></li>
            </ul>

        </div>
        <!--End sidebar-wrapper-->
        <!--Start topbar header-->
        <header class="topbar-nav">
            <nav class="navbar navbar-expand fixed-top bg-danger">
                <ul class="navbar-nav mr-auto align-items-center">
                    <li class="nav-item">
                        <a class="nav-link toggle-menu" href="javascript:void();">
                            <i class="icon-menu menu-icon"></i>
                        </a>
                    </li>
                    <li class="nav-item">
                        <form class="search-bar">
                            <input type="text" class="form-control" placeholder="Enter keywords">
                            <a href="javascript:void();"><i class="icon-magnifier"></i></a>
                        </form>
                    </li>
                </ul>

                <ul class="navbar-nav align-items-center right-nav-link">
                    @Html.Action("Navbar", "Header")
                </ul>
            </nav>
        </header>
        <!--End topbar header-->

        <div class="clearfix"></div>

        <div class="content-wrapper">
            <div class="container-fluid">

                <div class="row pt-2 pb-2">
                    <div class="col-sm-9">
                        <h4 class="page-title">@ViewBag.Title</h4>
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="@Url.Action("Index","Panel")">Dashboard</a></li>
                            <li class="breadcrumb-item"><a href="@Url.Action("Index")">@this.ViewContext.RouteData.Values["controller"].ToString()</a></li>
                            <li class="breadcrumb-item active" aria-current="page">@this.ViewContext.RouteData.Values["action"].ToString()</li>
                        </ol>
                    </div>
                </div>

                <!--Start Dashboard Content-->

                @RenderBody()

                <!--End Dashboard Content-->

            </div>
            <!-- End container-fluid-->

        </div><!--End content-wrapper-->
        <!--Start Back To Top Button-->
        <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>
        <!--End Back To Top Button-->
        <!--Start footer-->
        <footer class="footer">
            <div class="container">
                <div class="text-center">
                    Copyright © 2018 DashRock Admin
                </div>
            </div>
        </footer>
        <!--End footer-->

    </div><!--End wrapper-->
    <!-- Bootstrap core JavaScript-->
    <script src="~/Content/color-admin/assets/js/jquery.min.js"></script>
    <script src="~/Content/color-admin/assets/js/popper.min.js"></script>
    <script src="~/Content/color-admin/assets/js/bootstrap.min.js"></script>

    <!-- simplebar js -->
    <script src="~/Content/color-admin/assets/plugins/simplebar/js/simplebar.js"></script>
    <!-- waves effect js -->
    <script src="~/Content/color-admin/assets/js/waves.js"></script>
    <!-- sidebar-menu js -->
    <script src="~/Content/color-admin/assets/js/sidebar-menu.js"></script>
    <!-- Custom scripts -->
    <script src="~/Content/color-admin/assets/js/app-script.js"></script>

    <!-- Full Calendar -->
    <script src='~/Content/color-admin/assets/plugins/fullcalendar/js/moment.js'></script>
    <script src='~/Content/color-admin/assets/plugins/fullcalendar/js/fullcalendar.min.js'></script>
    <script>
        $(document).ready(function () {
            $('#calendar').fullCalendar({
                header: {
                    left: 'prev,next today',
                    center: 'title',
                    right: 'month,agendaWeek,agendaDay'
                },
                editable: true,
                eventLimit: true, // allow "more" link when too many events
                events: "/Calendar/Events"
            });

        });

    </script>
    <!-- Vector map JavaScript -->
    <script src="~/Content/color-admin/assets/plugins/vectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="~/Content/color-admin/assets/plugins/vectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- Sparkline JS -->
    <script src="~/Content/color-admin/assets/plugins/sparkline-charts/jquery.sparkline.min.js"></script>
    <!-- Chart js -->
    <script src="~/Content/color-admin/assets/plugins/Chart.js/Chart.min.js"></script>
    <!--notification js -->
    <script src="~/Content/color-admin/assets/plugins/notifications/js/lobibox.min.js"></script>
    <script src="~/Content/color-admin/assets/plugins/notifications/js/notifications.min.js"></script>
    <!-- Index js -->
    <script src="~/Content/color-admin/assets/js/index2.js"></script>

</body>
</html>
