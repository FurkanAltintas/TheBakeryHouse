@model EntityLayer.Concrete.Address

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Dashboard/Views/Shared/_LayoutDashboard.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header text-uppercase">@Model.Home @Model.HomeSubTitle</div>
            <div class="card-body">
                <iframe src="@Model.Map" style="border:0; width: 100%; height: 600px;" allowfullscreen="" loading="lazy"></iframe>
            </div>
        </div>
    </div>
</div>