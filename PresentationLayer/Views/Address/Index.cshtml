@model EntityLayer.Concrete.Address

    <div class="col-lg-3">
        <div class="contact_info">
            <div class="info_item">
                <i class="lnr lnr-home"></i>
                <h6>@Model.Home</h6>
                <p>@Model.HomeSubTitle</p>
            </div>
            <div class="info_item">
                <i class="lnr lnr-phone-handset"></i>
                <h6><a href="#">@Model.Phone</a></h6>
                <p>@Model.PhoneSubTitle</p>
            </div>
            <div class="info_item">
                <i class="lnr lnr-envelope"></i>
                <h6><a href="#">@Model.Email</a></h6>
                <p>@Model.EmailSubTitle</p>
            </div>
        </div>
    </div>
